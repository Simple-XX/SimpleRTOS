#编译命令指定
#TOOLCHAIN_DIR = $(shell pwd)
CROSS_COMPILE = $(BUILD_DIR)/../gcc-arm-none-eabi-10-2020-q4-major/bin/arm-none-eabi-

CC = $(CROSS_COMPILE)gcc
AS = $(CROSS_COMPILE)gcc
AR = $(CROSS_COMPILE)ar
LD = $(CROSS_COMPILE)gcc
CP = $(CROSS_COMPILE)objcopy
SZ = $(CROSS_COMPILE)size

#添加参数使LD输出.map文件:-Wl,-Map=xx.map
LIST_MAP_FILE = -Wl,-Map

#规则目录指定
RULE_DIR = $(BUILD_DIR)/compiler_configs/arm_none_gcc

#最终目标文件名
MAP_NAME := $(PROJECT).map
ELF_NAME := $(PROJECT).elf
BIN_NAME := $(PROJECT).bin
HEX_NAME := $(PROJECT).hex

.EXPORT_ALL_VARIABLES: